{
  "name": "@xtineui/theme",
  "description": "A collection of theme for XtineUI",
  "version": "8.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  ".": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0"
  },
  "keywords": [
    "xtineui"
  ],
  "author": "xtineui",
  "devDependencies": {
    "@xtineui/base": "workspace:*",
    "@xtineui/config-eslint": "workspace:*",
    "@xtineui/config-typescript": "workspace:*",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "@xtineui/base": "*"
  }
}
